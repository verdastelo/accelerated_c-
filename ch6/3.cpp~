/*
    Demonstrates how to use a reference parameter.
*/

#include <iostream> 

int f(int i); 

int main()
{
    int val = 1; 
    std::cout << "Old value for val: " 
              << val << std::endl; 
    
    std::cout << f(val) << '\n'; 
    
    std::cout << "New value for val: "
              << val << std::endl; 
              
    return 0;
}

void f (int i)
{
    i = 10;  
}

