/*
    A swap function created using reference parameters.
*/

#include <iostream> 

void swap (int &x, int&y)
{
    int temp = x; 
    x = y;
    y = temp;
}

int main()
{
    int i = 10, j = 20; 
    std::cout << "Initial values of i and j are: " 
              << i << " and " << j
              << std::endl;
              
    swap (i, j);
    std::cout << "Swapped values of i and j are: " 
              << i << " and " << j
              << std::endl; 
    
    return 0;
}
