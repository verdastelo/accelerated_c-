#include <iostream> 

using namespace std; 
// 印度和中国
int main() {
    wchar_t wc1; 
    wc1 = L'印'; 
    
    wchar_t wc2; 
    wc2 = L'中'; 
    cout << wc1 << endl; 
    cout << wc2 << endl; 
    
    if (wc1 > wc2) {
        cout << wc2-wc1 << endl; 
        }
    if (wc2 > wc1) {
        cout << wc1-wc2 << endl; 
        }
    
    return 0; 
    }
