/*
    What does the following program print? 
    */ 
    
#include <iostream> 

int main() {
    for (int i = 0; i < 10; i++) {
        std::cout << i << " "; 
        if (!(i % 2)) continue; 
        std::cout << "\n"; 
        }
    std::cout << "This program prints pairs of numbers. \n" 
              << "Each pair starts with an odd number. \n"
              << "EDIT: Each pair starts with an even number. \n"  
              << std::endl;
    return 0; 
    }
