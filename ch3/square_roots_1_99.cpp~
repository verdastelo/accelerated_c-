#include <iostream> 
#include <cmath> 

using namespace std; 

int main(void) {
    double sq_root_num = 0.0; 
    
    for (int num = 0; num <= 100; num++) {
        sq_root_num = sqrt((double) num); 
        cout << num << " " << sq_root_num << endl; 
        } 
        
        return 0; 
        
    } 
