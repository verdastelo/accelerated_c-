#include <iostream> 
#include <cmath> 

using namespace std; 

int main() {
    bool p, q; 
    p = true; 
    q = true; 
    
    cout << p << " XOR " << q << " is " << 
        ( ( p || q) && !(p && q)) << endl; 
        
    p = false; 
    q = true; 
    
    cout << p << " XOR " << q << " is " << 
        ( ( p || q) && !(p && q)) << endl; 
        
    p = true; 
    q = false;
    
    cout << p << " XOR " << q << " is " << 
        ( ( p || q) && !(p && q)) << endl;
    
    p = false; 
    q = false; 
    
    cout << p << " XOR " << q << " is " << 
        ( ( p || q) && !(p && q)) << endl;
        
    float a, b; 
    
    a = 10; 
    b = 3; 
    
    cout << a%b << endl; 
        
    return 0; 
    }
